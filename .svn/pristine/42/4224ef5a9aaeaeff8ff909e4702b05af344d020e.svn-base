package excecao;

public class ConflitoAgendamentoException extends RuntimeException {
	
	private static final long serialVersionUID = 1L;

	public ConflitoAgendamentoException(Exception e) {
		super(e);
	}

	public ConflitoAgendamentoException(String msg) {
		super(msg);		
	}

	public ConflitoAgendamentoException() {
		super("Já existe um agendamento para esta sala");		
	}

}
